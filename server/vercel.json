{
    "version": 2,
    "builds": [
        {
            "src": "server",
            "use": "@vercel/node",
            "config": {
                "buildCommand": "npm run vercel-postbuild && node index.js",
                "outputDirectory": "client/build"
            }
        }


    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ]
}